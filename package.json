{
    "name": "Stean",
    "description": "SensorThings Enhanced API Node",
    "author": "Adam Mario",
    "license": "ISC",
    "version": "0.9.6",
    "lockfileVersion": 1,
    "requires": true,
    "repository": {
        "type": "git",
        "url": "https://github.com/Mario-35/Stean"
    },
    "scripts": {
        "------DEV-----": "",
        "find-deadcode": "ts-prune",
        "dev": "nodemon --inspect ./src/server/index.ts",
        "debug": "nodemon",
        "lint": "tsc --noEmit && eslint \"./src/server/**/*.{js,ts}\" --quiet --fix",
        "prettier-format": "prettier --config .prettierrc ./src/server/**/*.ts --write",
        "------BUILD-----": "",
        "buildDev": "npm run build-ts && node build.js dev",
        "buildRemDev": "npm run build-ts && node build.js dev && scp -r -p builds/stean_latest.zip madam@194.167.76.143:./",
        "buildRem": "npm run build-ts && node build.js && scp -r -p builds/stean_latest.zip madam@194.167.76.143:./",
        "buildDocker": "npm run build-ts && node build.js docker && docker-compose up --build -d",
        "build": "npm run build-ts && node build.js",
        "git": "git add ./src/ ./package.json ./realease.md ./dist.zip ./scripts/ ./scripts/ ./README.md ",
        "build-ts": "rimraf ./build && tsc",
        "serve": "node build/index.js",
        "upProd": "npm run build-ts && node build.js && git add ./src/ ./package.json ./realease.md ./dist.zip ./scripts/ ./README.md && git commit --file .git/COMMIT_EDITMSG && git push",
        "upDev": "npm run build-ts && node build.js dev && git add ./src/ ./package.json ./realease.md ./dist.zip ./scripts/ ./README.md && git commit --file .git/COMMIT_EDITMSG && git push",
        "------TEST-------": "",
        "test-all": "mocha -r ts-node/register src/server/**/*.spec.ts",
        "test": "set NODE_ENV=test && rimraf ./src/test/apidoc rimraf ./src/test/tests.md && rimraf ./src/test/apiDocs && node node_modules/mocha/bin/mocha -r ts-node/register src/test/integration/**/*.spec.ts",
        "------API Doc-------": "",
        "doc": "node bin/apidoc -v -i src/ -o apidoc/",
        "------Docker-------": "",
        "docker": "docker-compose up --build -d"
    },
    "keywords": [],
    "dependencies": {
        "@koa/cors": "^3.4.3",
        "aedes": "^0.51.2",
        "aedes-server-factory": "^0.2.1",
        "async-mqtt": "^2.6.3",
        "busboy": "^0.3.1",
        "cookie": "^0.6.0",
        "cookie-parser": "^1.4.6",
        "jsonwebtoken": "^9.0.2",
        "koa": "^2.3.0",
        "koa-bodyparser": "^4.3.0",
        "koa-favicon": "^2.1.0",
        "koa-helmet": "^6.1.0",
        "koa-json": "^2.0.2",
        "koa-jwt": "^4.0.3",
        "koa-logger": "^3.2.1",
        "koa-router": "^10.0.0",
        "koa-static": "^5.0.0",
        "postgres": "^3.4.4"
    },
    "devDependencies": {
        "@node-minify/clean-css": "^6.2.0",
        "@node-minify/cli": "^8.0.6",
        "@node-minify/core": "^6.2.0",
        "@node-minify/html-minifier": "^6.2.0",
        "@node-minify/jsonminify": "^6.2.0",
        "@node-minify/uglify-js": "^6.2.0",
        "@types/apidoc": "^0.50.0",
        "@types/busboy": "^0.2.3",
        "@types/chai": "^4.2.14",
        "@types/cookie": "^0.5.1",
        "@types/cookie-parser": "^1.4.3",
        "@types/fs-extra": "^11.0.4",
        "@types/html-minifier-terser": "^7.0.0",
        "@types/jsonwebtoken": "^8.5.8",
        "@types/koa": "^2.11.6",
        "@types/koa__cors": "^5.0.0",
        "@types/koa-bodyparser": "^4.3.0",
        "@types/koa-favicon": "^2.1.3",
        "@types/koa-html-minifier": "^1.0.1",
        "@types/koa-json": "^2.0.18",
        "@types/koa-logger": "^3.1.1",
        "@types/koa-passport": "^4.0.2",
        "@types/koa-router": "^7.4.1",
        "@types/koa-static": "^4.0.2",
        "@types/mocha": "^9.0.0",
        "@types/nodemailer": "^6.4.4",
        "@types/pg": "^8.6.1",
        "@types/superagent": "^4.1.10",
        "@typescript-eslint/parser": "^8.2.0",
        "apidoc": "^0.50.2",
        "archiver": "^5.2.0",
        "chai": "^4.2.0",
        "chai-http": "^4.3.0",
        "eslint": "^8.4.1",
        "eslint-config-prettier": "^8.3.0",
        "eslint-plugin-prettier": "^4.0.0",
        "mocha": "^9.1.3",
        "mqtt": "^5.8.1",
        "nodemon": "^2.0.7",
        "prettier": "^2.5.1",
        "rimraf": "^3.0.2",
        "ts-node": "^10.9.1",
        "ts-prune": "^0.10.3",
        "typescript": "^5.4.5",
        "uglify-js": "^3.17.4"
    }
}

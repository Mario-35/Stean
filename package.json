{
    "name": "Stean",
    "description": "SensorThings Enhanced API Node",
    "author": "Adam Mario",
    "license": "ISC",
    "version": "1.3.0",
    "lockfileVersion": 1,
    "requires": true,
    "repository": {
        "type": "git",
        "url": "https://github.com/Mario-35/Stean"
    },
    "scripts": {
        "------DEV-----": "",
        "find-deadcode": "ts-prune",
        "dev": "nodemon --inspect ./src/server/index.ts",
        "devDebug": "nodemon --inspect ./src/server/index.ts debug",
        "debug": "nodemon",
        "lint": "tsc --noEmit && eslint \"./src/server/**/*.{js,ts}\" --quiet --fix",
        "prettier-format": "prettier --config .prettierrc ./src/server/**/*.ts --write",
        "------BUILD-----": "",
        "buildDev": "npm run build-ts && node build.js dev",
        "buildRemDev": "npm run build-ts && node build.js dev && scp -r -p builds/stean_latest.zip madam@194.167.76.143:./",
        "buildRem": "npm run build-ts && node build.js && scp -r -p builds/stean_latest.zip madam@194.167.76.143:./",
        "buildDocker": "npm run build-ts && node build.js docker && docker-compose up --build -d",
        "build": "npm run build-ts && node build.js",
        "git": "git add ./src/ ./package.json ./realease.md ./dist.zip ./scripts/ ./scripts/ ./README.md ",
        "build-ts": "rimraf ./build && tsc",
        "serve": "node build/indexgit status.js",
        "upProd": "npm run build-ts && node build.js && git add -u && git commit --file .git/COMMIT_EDITMSG && git push",
        "upDev": "npm run build-ts && node build.js dev && git add -u && git commit --file .git/COMMIT_EDITMSG && git push",
        "------TEST-------": "",
        "test-all": "mocha -r ts-node/register src/server/**/*.spec.ts",
        "test": "set NODE_ENV=test && rimraf ./src/test/tests.md && node node_modules/mocha/bin/mocha -r ts-node/register src/test/integration/**/*.spec.ts",
        "redo": "rimraf node_modules/ && npm install",
        "docker": "docker-compose up --build -d"
    },
    "keywords": [],
    "dependencies": {
        "@koa/cors": "^5.0.0",
        "aedes": "^0.51.3",
        "aedes-server-factory": "^0.2.1",
        "async-mqtt": "^2.6.3",
        "busboy": "^0.3.1",
        "cookie": "^0.6.0",
        "cookie-parser": "^1.4.6",
        "jsonwebtoken": "^9.0.2",
        "koa": "^3.1.1",
        "koa-bodyparser": "^4.4.1",
        "koa-favicon": "^2.1.0",
        "koa-helmet": "^8.0.1",
        "koa-jwt": "^4.0.3",
        "koa-logger": "^4.0.0",
        "koa-router": "^14.0.0",
        "koa-static": "^5.0.0",
        "postgres": "^3.4.7"
    },
    "devDependencies": {
        "@node-minify/clean-css": "^9.0.1",
        "@node-minify/cli": "^9.0.2",
        "@node-minify/core": "^9.0.2",
        "@node-minify/html-minifier": "^9.0.1",
        "@node-minify/jsonminify": "^9.0.1",
        "@node-minify/uglify-js": "^9.0.1",
        "@types/busboy": "^0.2.3",
        "@types/chai": "^4.2.14",
        "@types/cookie": "^0.6.0",
        "@types/cookie-parser": "^1.4.10",
        "@types/fs-extra": "^11.0.4",
        "@types/html-minifier-terser": "^7.0.0",
        "@types/jsonwebtoken": "^9.0.10",
        "@types/koa": "^3.0.1",
        "@types/koa__cors": "^5.0.1",
        "@types/koa__router": "^12.0.5",
        "@types/koa-bodyparser": "^4.3.13",
        "@types/koa-favicon": "^2.1.4",
        "@types/koa-html-minifier": "^1.0.1",
        "@types/koa-logger": "^3.1.1",
        "@types/koa-passport": "^6.0.3",
        "@types/koa-router": "^7.4.9",
        "@types/koa-static": "^4.0.2",
        "@types/mocha": "^9.0.0",
        "@types/pg": "^8.15.6",
        "@types/prismjs": "^1.26.5",
        "@typescript-eslint/parser": "^8.2.0",
        "archiver": "^7.0.1",
        "chai": "^4.2.0",
        "chai-http": "^4.3.0",
        "eslint": "^9.39.1",
        "eslint-plugin-prettier": "^5.5.4",
        "globby": "^11.0.4",
        "mocha": "^9.1.3",
        "mqtt": "^5.8.1",
        "nodemon": "^3.1.11",
        "prettier": "^3.6.2",
        "prismjs": "^1.30.0",
        "rimraf": "^6.1.0",
        "superagent": "^10.2.3",
        "ts-node": "^10.9.1",
        "ts-prune": "^0.10.3",
        "typescript": "^5.4.5",
        "uglify-js": "^3.17.4"
    }
}
